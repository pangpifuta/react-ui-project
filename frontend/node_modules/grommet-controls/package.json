{
  "name": "grommet-controls",
  "version": "1.1.17",
  "main": "index.js",
  "module": "es6/index.js",
  "jsnext:main": "es6/index.js",
  "description": "grommet 2 controls",
  "sideEffects": false,
  "authors": [
    "Atanas Stoyanov"
  ],
  "keywords": [
    "grommet",
    "react",
    "grommet-next",
    "react ui"
  ],
  "homepage": "https://grommet-nextjs.herokuapp.com/add-ons",
  "bugs": "https://github.com/atanasster/grommet-controls/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atanasster/grommet-controls.git"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 6.11.5"
  },
  "scripts": {
    "build": "webpack --mode production && babel ./src/js/ --ignore '__tests__' --out-dir ./dist && cross-env BABEL_ENV=es6 babel ./src/js/ --ignore '__tests__' --out-dir ./dist/es6",
    "generate-readme": "babel-node ./tools/generate-readme",
    "check": "cross-env NODE_ENV=test grommet check",
    "test": "grommet check -t",
    "test-update": "grommet check -t -- --updateSnapshot",
    "test-watch": "grommet check -t -- --watchAll",
    "pack": "babel-node ./tools/pack",
    "pub": "pushd ./ && npm run build && cd dist && npm publish && popd"
  },
  "peerDependencies": {
    "grommet": ">= 2.2.1",
    "grommet-icons": ">= 4.1.0",
    "react": ">= 16.7.0",
    "react-dom": ">= 16.7.0",
    "styled-components": ">= 4.1.3"
  },
  "dependencies": {
    "chartjs": "^0.3.24",
    "markdown-to-jsx": "^6.6.6",
    "node-emoji": "^1.8.1",
    "polished": "^2.0.3",
    "prop-types": "^15.5.10",
    "react-desc": "^4.1.2",
    "recompose": "^0.30.0",
    "text-mask-addons": "^3.7.2",
    "text-mask-core": "^5.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-styled-components": "^1.6.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.24.1",
    "chart.js": "^2.7.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css": "^2.2.1",
    "del": "^3.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "fs-extra": "^4.0.0",
    "grommet": "^2.2.1",
    "grommet-cli": "5.1.0",
    "grommet-icons": "^4.1.0",
    "jest-styled-components": "^6.2.0",
    "pre-commit": "^1.2.2",
    "react": "^16.7.0",
    "react-chartjs-2": "^2.7.4",
    "react-dev-utils": "^7.0.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "require-reload": "^0.2.2",
    "simple-git": "^1.74.1",
    "styled-components": "^4.1.3",
    "tarball-extract": "^0.0.6",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.7"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/js/components/**/*.{js}",
      "src/js/themes/**/*.{js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|templates|node_modules)[/\\\\]"
    ]
  },
  "pre-commit": [
    "check"
  ]
}
